    "editor.tokenColorCustomizations": {
        "[One Monokai]": {
            "textMateRules": [
                {
                    "name": "Loop",
                    "scope":"keyword.control.for",
                    "settings": {
                        "foreground": "#9f7063",
                    }
                },
                {
                    "name": "Semicolon",
                    "scope":"punctuation.terminator.statement",
                    "settings": {
                        "foreground": "#fb00ff",
                        "fontStyle": "italic bold underline strikethrough"
                    }
                },
                {
                    "name": "Namespace",
                    "scope":"entity.name.namespace",
                    "settings": {
                        "foreground": "#71f1fa",
                    }
                },
                {
                    "name": "Functions",
                    "scope":"entity.name.function",
                    "settings": {
                        "foreground": "#00a2ff",
                    }
                },
                {
                    "name": "Class",
                    "scope":"entity.name.type",
                    "settings": {
                        "foreground": "#f48d61",
                    }
                },
                {
                    "name": "Keyword",
                    "scope":"keyword",
                    "settings": {
                        "foreground": "#6af386",
                    }
                },
                {
                    "name": "Property",
                    "scope":"meta.body.class",
                    "settings": {
                        "foreground": "#f48d61",
                        "fontStyle": "bold"
                    }
                },
                {
                    "name": "Template <>",
                    "scope":"punctuation.section.angle-brackets",
                    "settings": {
                        "foreground": "#ff0000" }
                }
            ]
        }
    },
    "editor.semanticTokenColorCustomizations": {
        "[One Monokai]": {
            "enabled": true,
            "rules": {
                "namespace": "#71f1fa",
                "function": "#00a2ff",
                "method": "#00a2ff",
                "property": "#00ffd5",
                "variable": "#ffff",

            }
        }
    }
    
    
